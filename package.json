{
  "name": "@indigov/sparkpost",
  "version": "2.1.8",
  "description": "A Node.js wrapper for interfacing with your favorite SparkPost APIs",
  "main": "./lib/sparkpost.js",
  "types": "./index.d.ts",
  "scripts": {
    "audit:fix": "npx yarn-audit-fix",
    "coveralls": "cat ./test/reports/lcov.info | coveralls",
