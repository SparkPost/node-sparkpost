  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sparkpost": "1.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^10.3.0",
    "nock": "^9.0.0",
    "prettier": "^2.8.0",
    "sinon": "^1.17.5",
